{
  "services": [
    {
      "name": "kafka",
      "dockerfilePath": "kafka/Dockerfile",
      "envVars": {
        "KAFKA_LOG_DIRS": "/var/lib/kafka/data"
      }
    },
    {
      "name": "kafka-ui",
      "dockerfilePath": "kafka-ui/Dockerfile",
      "envVars": {
        "KAFKA_BROKER": "kafka:9092"
      }
    }
  ]
}
